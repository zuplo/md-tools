{
  "name": "@zuplo/md-tools",
  "sideEffects": false,
  "version": "0.0.0",
  "description": "Docs utilities",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/zuplo/md-tools"
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./out.js",
      "require": "./out.js"
    }
  },
  "files": [
    "*"
  ],
  "types": "./index.d.ts",
  "author": "Zuplo",
  "license": "Copyright 2021",
  "scripts": {
    "test": "mocha",
    "build": "npm run build:cjs && npm run build:tsc",
    "build:tsc": "tsc --build",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node --minify --sourcemap --target=node14 --outfile=dist/out.js",
    "release": "run-release --owner zuplo --repo md-tools",
    "set-package-version": "set-package-version --path ./package.json --version"
  },
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "unist-util-index": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.19",
    "@zuplo/dev": "^1.38.0",
    "@zuplo/dev-tools": "^1.62.0",
    "esbuild": "^0.18.9",
    "typescript": "^5.2.2"
  }
}
